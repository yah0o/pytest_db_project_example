{
  "priority": 1,
  "request": {
    "method": "POST",
    "urlPattern": "/catalog/api/v1/prepare",
    "bodyPatterns": [{
        "matchesJsonPath": "$..[?(@.catalog_code=~/ru.prepare_301-MAIN-\\d*/i)]"
    }]
  },
  "response": {
    "status": 301,
    "headers": {
      "location": "http://www.wargamint.net"
    }
  }
}
